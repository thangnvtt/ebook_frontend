{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "copy": "cp -rf ./src/assets/plugins ./dist/assets/",
    "start": "concurrently \"npm run build && npm run copy\" \"node index\""
  },
  "dependencies": {
    "concurrently": "^6.0.2",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "serve-static": "^1.14.1",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.1.3",
    "vite": "^2.2.3",
    "vue-tsc": "^0.0.24"
  }
}